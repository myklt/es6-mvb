  },
  "homepage": "https://github.com/myklt/es6-mvb#readme",
  "devDependencies": {
    "chromedriver": "^2.27.2",
    "cross-env": "^3.1.4",
    "cucumber": "^2.0.0-rc.7",
    "cucumber-html-reporter": "^0.3.7",
    "geckodriver": "^1.4.0",
    "minimist": "^1.2.0",
    "nightwatch": "^0.9.12",
    "nightwatch-cucumber": "^7.0.9",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.5.4",
    "selenium-server": "^3.0.1",
