{
  "name": "es6-mvb",
  "version": "0.0.1",
  "description": "ES6 Minimal Viable Boilerplate",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "test": "echo \"TODO: specify test\" && exit 0",
    "pretest:e2e": "npm run test:e2e-clean && mkdirp test/e2e/logs",
    "test:e2e": "cd test/e2e && cross-env NODE_ENV=test node runner.js --env chrome,firefox,default",
    "test:e2e-clean": "rimraf test/e2e/reports && rimraf test/e2e/logs && rimraf test/e2e/screenshots"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/myklt/es6-mvb.git"
  },
  "keywords": [
    "ES6"
  ],
